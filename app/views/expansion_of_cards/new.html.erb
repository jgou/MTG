<h1>New expansion_of_card</h1>

<% form_for(@expansion_of_card) do |f| %>
  <%= f.error_messages %>
	<table>
	<tr>
  <td>
    <%= f.label :card_id %>
	</td>
	<td>
   	<%= collection_select(:expansion_of_card, :card_id, Card.all, :id, :name) %>
	</td>
  </tr>
	<tr>
	<td>
    <%= f.label :expansion_id %>
	</td>
	<td>
		<%= collection_select(:expansion_of_card, :expansion_id, Expansion.all, :id, :name) %>
 	</td>
	</tr>
	</table>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', expansion_of_cards_path %>
